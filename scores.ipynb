    "plt.subplot(222); plt.imshow(autocorr_pp,cmap='jet'); plt.title('\"Good\" Autocorrellogram')\n",
    "plt.subplot(223); plt.imshow(bplace[:,:,0], cmap='jet'); plt.title('\"Bad\" Place Cell')\n",
